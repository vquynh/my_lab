<!DOCTYPE html>
<html>

  <body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <% if logged_in?(:labor_staff) %>
      <%= render "shared/admin_application_nav"%>
    <% else%>
      <%= render "shared/application_nav"%>
    <% end %>

    <%= login_helper %>
    <%= yield %>

    <% if session[:source] %>
      <p> Thanks for visiting Mylab from <%= session[:source] %> </p>
    <% end %>
  </body>
</html>
